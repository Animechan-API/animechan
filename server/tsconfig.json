{
	"compilerOptions": {
		"target": "ES2020",
		"module": "commonjs",
		"resolveJsonModule": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true,
		"baseUrl": "./",
		"paths": {
			"~/*": [
				"./*"
			]
		}
	},
	"include": [
		"**/*.ts",
		"data/*.json"
	]
}
